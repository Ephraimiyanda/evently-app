{
  "expo": {
    "name": "Evently",
    "slug": "Evently",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "Streamline event planning, creating simplicity and efficiency making event planning effortless, plan better, stress less.",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.evently.app",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#0ea5e9",
          "dark": {
            "image": "./assets/images/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#0ea5e9"
          }
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-mail-composer"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0d11d893-3a06-438c-a199-6d278f3ab676"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/0d11d893-3a06-438c-a199-6d278f3ab676"
    }
  }
}
